{
  "name": "rshare-tpm",
  "version": "1.0.0",
  "description": "The web app of TPM for rShare.",
  "main": "index.js",
  "scripts": {
    "dev_webpack": "webpack-dev-server --hot --config ./app/build/webpack.dev.js",
    "dev_server": "cross-env NODE_ENV=development nodemon --harmony ./server/app.js",
    "prod_webpack": "webpack --config ./app/build/webpack.prod.js",
    "prod_server": "cross-env NODE_ENV=production node ./server/app.js",
    "prod": "npm run prod_webpack && npm run prod_server"
  },
  "keywords": [
    "node",
    "koa",
    "react",
    "webpack"
  ],
  "author": "BeckyWang",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "thrift": "^0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.12",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
